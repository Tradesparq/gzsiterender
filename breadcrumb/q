<script type="text/javascript">
var base_url = "http://www.tradesparq.com/";

//解决IE6兼容trim()
if(typeof String.prototype.trim !== 'function'){   
	String.prototype.trim = function(){
		return this.replace(/^\s+|\s+$/g, ''); 
	};
}
$(function(){
	$(".public_search").live('click', function(){
		var keyword = $.trim($("input[name=public_keyword]").val());
		keyword = keywordProcess(keyword);

		var lang = "english";
		var url = "http://www.tradesparq.com/catalog/all/" + encodeURI(keyword) + "-manufacturers";
		
		if(lang == 'chinese')
		{
			url += "?lang="+lang;
		}
		window.location.href = url; 
		return false;
	});
	
	$("input[name=public_keyword]").live("keydown",function(event){
		if(event.keyCode==13){
			if ((window.location.href).indexOf('Customs-Data') == -1) {
				jQuery('.public_search').click();
			} else {
				jQuery('#searchCustoms').click();
			}
			return false;
		}
	});
	
	$("#searchCustoms").live('click', function(){
		var keyword = $.trim($("input[name=public_keyword]").val());
		keyword = keywordProcess(keyword);

		var lang = "english";
		if ((window.location.href).indexOf('/Import-') == -1) {
			var url = "http://www.tradesparq.com/Customs-Data/Export-" + encodeURI(keyword);
		} else {
			var url = "http://www.tradesparq.com/Customs-Data/Import-" + encodeURI(keyword);
		}
		
		if(lang == 'chinese')
		{
			url += "?lang="+lang;
		}
		window.location.href = url; 
		return false;
	});
	
	function keywordProcess(keyword)
	{
		if(!is_empty(keyword))
		{
			keyword = keyword.replace(/\b\w+\b/g, function(keyword){
				  return keyword.substring(0,1).toUpperCase()+keyword.substring(1);}
				  );
			//Replace single hyphen with '--' and replace blank character with '-'
			keyword = keyword.replace(/-+/g, "--");
			keyword = keyword.replace(/\s+/g, "-");
			keyword = keyword.replace(/-{3,}/g, "--");
		}
		
		return keyword;
	}
});
